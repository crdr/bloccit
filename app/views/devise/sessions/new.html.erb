<h2>Sign in</h2>

<div class="row">
  <div class="col-md-8"
    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= devise_error_messages! %>
      <dive class=:form-group">
        <%= f.label :name %>
        <%= f.text_field :name, autofocus: true, class: 'form-control', placeholder: "Enter email" %>
        
</div>
